<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="mimi"
        Arguments=""
        InstallLocation="C:\Program Files\goburn\Mimi">
    </Installer>
    <PackageInformation
        PackageName="Mimi"
        PackageDisplayName="Mimi"
        PublisherName="CN=goburn"
        PublisherDisplayName="goburn"
        Version="0.1.0.0"
        PackageDescription="My Product Description">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.goburn.mimi"
                Description="My Product Description"
                DisplayName="Mimi"
                ExecutableName="mimi"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>Mimi</DisplayName>
            <PublisherDisplayName>goburn</PublisherDisplayName>
            <Description>My Product Description</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="mimi.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
